<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CellView">
        <attr name="cellColor" format="color" />
    </declare-styleable>

    <declare-styleable name="ColumnView">
        <attr name="backgroundColor" format="color" />
        <attr name="barNumber" format="integer" />
    </declare-styleable>

    <declare-styleable name="OperationView">
        <attr name="operationBackgroundColor" format="color" />
        <attr name="buttonBackgroundColor" format="color" />
    </declare-styleable>

    <declare-styleable name="OperationButton">
        <attr name="buttonCircleColor" format="color" />
        <attr name="buttonImage" format="reference" />
        <attr name="buttonEnabled" format="boolean" />
        <attr name="buttonImageTint" format="color" />
    </declare-styleable>

    <declare-styleable name="SmallFAB">
        <attr name="fabCircleColor" format="color" />
        <attr name="fabImage" format="reference" />
        <attr name="fabEnabled" format="boolean" />
        <attr name="fabImageTint" format="color" />
    </declare-styleable>
</resources>